# Key nutritional parameters
nutr_params <- c("DM", "CP", "ADF", "NDF", "OM", "ME", "IVDMD")
# Calculate statistics by feed type
for(param in nutr_params) {
if(param %in% names(dt)) {
cat("--- ", param, " ---\n")
stats <- dt[!is.na(get(param)), .(
Count = .N,
Mean = round(mean(get(param), na.rm = TRUE), 2),
SD = round(sd(get(param), na.rm = TRUE), 2),
Min = round(min(get(param), na.rm = TRUE), 2),
Max = round(max(get(param), na.rm = TRUE), 2),
CV = round(sd(get(param), na.rm = TRUE) / mean(get(param), na.rm = TRUE) * 100, 1)
), by = `Feed type`][order(-Count)]
print(stats)
cat("\n")
}
}
